<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="net.worm.jshelf.user.sdk.model">
	<class name="User" table="T_user">
		<id name="uuid" column="uuid" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="name" column="name" type="java.lang.String"
			not-null=true />
		<property name="privilege" column="privilege" type="int"
			not-null=true />
		<property name="description" column="name" type="java.lang.String"
			not-null=true />
	</class>
</hibernate-mapping>